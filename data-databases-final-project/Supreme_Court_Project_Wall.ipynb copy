   "source": [
    "# Joining article properties\n",
    "\n",
    "ginsburg[\"article_cell\"] = ginsburg[\"name_date\"] + \"<br>\" + ginsburg[\"count_readable\"].map(str) + \"<br>\" + ginsburg[\"words_readable\"]"
