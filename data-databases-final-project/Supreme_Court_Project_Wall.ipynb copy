    "# you could try here--Or email me with questions..."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['\\n\\nCHIEF JUSTICE ROBERTS:', ' Counsel --\\n\\nMS. ROSS: -- thus resulting in --', '19-46']\n"
     ]
    }
   ],
   "source": [
    "all_cases = []\n",
    "for file in all_2019:\n",
    "    try:\n",
    "        name_pdf = file['pdf'].split('/')[-1]\n",
    "        name_txt = name_pdf.split('.')[0] + \".txt\"\n",
    "        #print(name_txt)\n",
    "        f = open('/Users/sheridanwall/Documents/Data/2019pdfs_official/'+name_txt, 'r')\n",
    "        each_transcript = f.read()\n",
    "        find_her = r\"Heritage Reporting Corporation[\\s\\n\\d-]+Official\"\n",
    "        clean_transcript = re.sub(find_her,\"\",each_transcript)\n",
    "        clean_transcripts1=re.split(r\"(\\nCHIEF JUSTICE ROBERTS:)\",clean_transcript,1)\n",
    "        split_transcripts = clean_transcripts1[1]+clean_transcripts1[2]\n",
    "        split_transcript1 = re.split(r\"(The case is submitted.)\", split_transcripts,1)\n",
    "        split_transcripts2 = split_transcript1[0]+split_transcript1[1]\n",
    "        find_speakers = r\"(\\n[A-Z\\s\\n]+:)\"\n",
    "        speaker_transcripts = re.split(find_speakers,split_transcripts2)\n",
    "        joined_dialogue1 = [[speaker_transcripts[x],speaker_transcripts[x+1],file['docket']] for x in range(1,len(speaker_transcripts),2)]\n",
    "        all_cases+=joined_dialogue1\n",
    "    except:\n",
    "        pass\n",
    "\n",
    "print(all_cases[1])"
   ]
  },
  {
