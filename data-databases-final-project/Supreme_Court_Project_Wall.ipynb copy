  {
   "cell_type": "code",
   "execution_count": 207,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Making top 5 words readable\n",
    "def transl(topwords):\n",
    "    word_string = \"The top 5 words are: \"\n",
    "    elements = topwords.split('||')\n",
    "    for element in elements:\n",
    "        words = element[1:-1].split(',')\n",
    "        if len(words) == 2:\n",
    "            word_string += words[0][1:-1] + \": \" + words[1] + \" occurances. \"\n",
    "    return word_string\n",
    "\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 209,
   "metadata": {},
   "outputs": [],
   "source": [
    "ginsburg['words_readable'] = ginsburg['most_used_words'].apply(lambda x: transl('||'.join(map(str, x))))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 212,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
